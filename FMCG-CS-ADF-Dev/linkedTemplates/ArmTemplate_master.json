{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "FMCG-CS-ADF-Dev"
		},
		"AzureDatabricks1_FMCG_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1_FMCG_LS'"
		},
		"AzureDatabricksDeltaLake1_FMCG_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1_FMCG_LS'"
		},
		"Sftp_FMCG_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp_FMCG_LS'"
		},
		"SqlServer_fmcg_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_fmcg_ls'",
			"defaultValue": "Integrated Security=True;Data Source=MBN\\SQLEXPRESS;Initial Catalog=cleverstudies"
		},
		"SqlServer_fmcg_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_fmcg_ls'"
		},
		"AzureDataLakeStorage1_FMCG_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fmcgadlsdev.dfs.core.windows.net/"
		},
		"AzureDatabricks1_FMCG_LS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1214-074043-ytm6u0gp"
		},
		"AzureKeyVault1_FMCG_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fmcgkv.vault.azure.net/"
		},
		"Sftp_FMCG_LS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "192.168.1.103"
		},
		"Sftp_FMCG_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "meghana_bn@outlook.com"
		},
		"SqlServer_fmcg_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "meghana_bn@outlook.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "FMCG-CS-ADF-Dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_FMCG_LS_accessToken": {
						"value": "[parameters('AzureDatabricks1_FMCG_LS_accessToken')]"
					},
					"AzureDatabricksDeltaLake1_FMCG_LS_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_FMCG_LS_accessToken')]"
					},
					"Sftp_FMCG_LS_password": {
						"value": "[parameters('Sftp_FMCG_LS_password')]"
					},
					"SqlServer_fmcg_ls_connectionString": {
						"value": "[parameters('SqlServer_fmcg_ls_connectionString')]"
					},
					"SqlServer_fmcg_ls_password": {
						"value": "[parameters('SqlServer_fmcg_ls_password')]"
					},
					"AzureKeyVault1_FMCG_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_FMCG_LS_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage1_FMCG_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_FMCG_LS_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_FMCG_LS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_FMCG_LS_properties_typeProperties_existingClusterId')]"
					},
					"Sftp_FMCG_LS_properties_typeProperties_host": {
						"value": "[parameters('Sftp_FMCG_LS_properties_typeProperties_host')]"
					},
					"Sftp_FMCG_LS_properties_typeProperties_userName": {
						"value": "[parameters('Sftp_FMCG_LS_properties_typeProperties_userName')]"
					},
					"SqlServer_fmcg_ls_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_fmcg_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}