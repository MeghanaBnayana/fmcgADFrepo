{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "FMCG-CS-ADF-Dev"
		},
		"AzureDatabricks1_FMCG_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1_FMCG_LS'"
		},
		"AzureDatabricksDeltaLake1_FMCG_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1_FMCG_LS'"
		},
		"Sftp_FMCG_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp_FMCG_LS'"
		},
		"SqlServer_fmcg_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_fmcg_ls'",
			"defaultValue": "Integrated Security=True;Data Source=MBN\\SQLEXPRESS;Initial Catalog=cleverstudies"
		},
		"SqlServer_fmcg_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_fmcg_ls'"
		},
		"AzureDataLakeStorage1_FMCG_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fmcgadlsdev.dfs.core.windows.net/"
		},
		"AzureDatabricks1_FMCG_LS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1214-074043-ytm6u0gp"
		},
		"AzureKeyVault1_FMCG_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fmcgkv.vault.azure.net/"
		},
		"Sftp_FMCG_LS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "192.168.1.103"
		},
		"Sftp_FMCG_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "meghana_bn@outlook.com"
		},
		"SqlServer_fmcg_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "meghana_bn@outlook.com"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ds_input_mssql')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "SqlServer_fmcg_ls",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"database_name": {
						"type": "string",
						"defaultValue": "cleverstudies"
					},
					"server_name": {
						"type": "string",
						"defaultValue": "MBN\\SQLEXPRESS"
					}
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [],
				"typeProperties": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/SqlServer_fmcg_ls')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_output_parquet')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStorage1_FMCG_LS",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"container_name": {
						"type": "string"
					},
					"adls_file_path": {
						"type": "string"
					},
					"table_name": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@dataset().table_name",
							"type": "Expression"
						},
						"folderPath": {
							"value": "@dataset().container_name",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().adls_file_path",
							"type": "Expression"
						}
					},
					"compressionCodec": "snappy"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeStorage1_FMCG_LS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage1_FMCG_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage1_FMCG_LS_properties_typeProperties_url')]"
				},
				"connectVia": {
					"referenceName": "FMCG-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/FMCG-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDatabricks1_FMCG_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://adb-1982240660738314.14.azuredatabricks.net",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('AzureDatabricks1_FMCG_LS_accessToken')]"
					},
					"existingClusterId": "[parameters('AzureDatabricks1_FMCG_LS_properties_typeProperties_existingClusterId')]"
				},
				"connectVia": {
					"referenceName": "FMCG-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/FMCG-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDatabricksDeltaLake1_FMCG_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricksDeltaLake",
				"typeProperties": {
					"domain": "https://adb-1982240660738314.14.azuredatabricks.net",
					"clusterId": "1214-074043-ytm6u0gp",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('AzureDatabricksDeltaLake1_FMCG_LS_accessToken')]"
					}
				},
				"connectVia": {
					"referenceName": "FMCG-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/FMCG-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureKeyVault1_FMCG_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('AzureKeyVault1_FMCG_LS_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Sftp_FMCG_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "Sftp",
				"typeProperties": {
					"host": "[parameters('Sftp_FMCG_LS_properties_typeProperties_host')]",
					"port": 22,
					"skipHostKeyValidation": true,
					"authenticationType": "Basic",
					"userName": "[parameters('Sftp_FMCG_LS_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Sftp_FMCG_LS_password')]"
					}
				},
				"connectVia": {
					"referenceName": "FMCG-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/FMCG-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SqlServer_fmcg_ls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('SqlServer_fmcg_ls_connectionString')]",
					"userName": "[parameters('SqlServer_fmcg_ls_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('SqlServer_fmcg_ls_password')]"
					}
				},
				"connectVia": {
					"referenceName": "FMCG-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/FMCG-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/FMCG-IR')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}